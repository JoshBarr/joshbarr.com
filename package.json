{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^2.28.1",
    "gatsby-awesome-pagination": "^0.3.6",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-feed": "^2.9.0",
    "gatsby-plugin-google-analytics": "^2.7.0",
    "gatsby-plugin-manifest": "^2.8.0",
    "gatsby-plugin-offline": "^3.6.0",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "gatsby-plugin-sharp": "^2.10.1",
    "gatsby-plugin-sitemap": "^2.8.0",
    "gatsby-remark-images": "^3.7.0",
    "gatsby-source-filesystem": "^2.7.0",
    "gatsby-transformer-remark": "^2.12.0",
    "gatsby-transformer-sharp": "^2.8.0",
    "gatsby-transformer-yaml": "^2.7.0",
    "joi": "^17.3.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "deploy": "gh-pages -d public --branch master",
    "func": "netlify-lambda build ./src/functions && git add ./functions && git commit -am'built functions' && git push",
    "ship": "gatsby build && gh-pages -d public --branch master",
    "develop:func": "run.env netlify-lambda serve ./src/functions"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "gh-pages": "^3.1.0",
    "lodash": "^4.17.20",
    "netlify-lambda": "^2.0.1",
    "prettier": "^2.2.1",
    "request": "^2.88.0",
    "run.env": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
